@article{damatoEnvironmentalRiskFactors2005,
  title = {Environmental Risk Factors and Allergic Bronchial Asthma},
  author = {D'Amato, G. and Liccardi, G. and D'Amato, M. and Holgate, S.},
  year = {2005},
  month = sep,
