@article{hoffmanEffectsHistoricalHousing2020,
  title = {The {{Effects}} of {{Historical Housing Policies}} on {{Resident Exposure}} to {{Intra}}-{{Urban Heat}}: {{A Study}} of 108 {{US Urban Areas}}},
  shorttitle = {The {{Effects}} of {{Historical Housing Policies}} on {{Resident Exposure}} to {{Intra}}-{{Urban Heat}}},
